<h1>Welcome to Embr!</h1>

  <select id="gender">
    <option value="male">male</option>
    <option value="female">female</option>
  </select>

<% @users.each do |user| %>

<h3>
  <a href="/users/<%= user.id %>"><%= user.name %></a>

</h3>
  <a href="/users/<%= user.id %>">
  <img width="380" height "380" src="<%= user.prof_img %>">
  </a>
<ul>
  <li><%= user.email %></li>
  <li><%= user.age %></li>
</ul>

<% end %>
