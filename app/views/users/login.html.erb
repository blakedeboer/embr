<%= render 'users/header' %>
<h1>Login</h1>

<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in", class: "btn btn-primary" %>
<% end %>
<h6>Don't have an account? <a href="/users/new">Sign up</a> </h6>

